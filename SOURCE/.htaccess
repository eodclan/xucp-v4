Options -Indexes

RewriteEngine On

ErrorDocument 400     /vendor/webcp/404/index.php
ErrorDocument 401     /vendor/webcp/404/index.php
ErrorDocument 402     /vendor/webcp/404/index.php
ErrorDocument 405     /vendor/webcp/404/index.php
ErrorDocument 403     /vendor/webcp/404/index.php
ErrorDocument 404     /vendor/webcp/404/index.php
ErrorDocument 500     /vendor/webcp/404/index.php
ErrorDocument 505     /vendor/webcp/505/index.php

# Ban Typical Vulnerability Scanners and others
# Kick out Script Kiddies
RewriteCond %{HTTP_USER_AGENT} ^()$ [NC,OR] # void of UserAgent
RewriteCond %{HTTP_USER_AGENT} ^(java|curl|wget).* [NC,OR]
RewriteCond %{HTTP_USER_AGENT} ^.*(libwww-perl|curl|wget|python|nikto|wkito|pikto|pykto|scan|acunetix|qualys|fuck|kiss|ass|Morfeus|0wn|hack|h4x|h4x0r).* [NC,OR]
RewriteCond %{HTTP_USER_AGENT} ^.*(winhttp|HTTrack|clshttp|archiver|loader|email|harvest|extract|grab|miner).* [NC,OR]

# Block directory traversal Requests trying to get .. or /.. stuff
RewriteCond %{QUERY_STRING} (\.\./|/\.\.|\./\.)
RewriteRule .* - [F,NS,L]
# Block INSTALL.php requests
RewriteCond %{REQUEST_URI} (install|setup|config|configure)\.php [NC]
RewriteRule ^(.*)$ index.php [F,L]

